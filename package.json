{
  "name": "mui-symbols-material",
  "version": "0.2.0",
  "private": true,
  "author": "Aleksandr Rubtsov",
  "description": "Material Design symbols distributed as SVG React components.",
  "main": "./src/index.js",
  "keywords": [
    "react",
    "react-component",
    "mui",
    "material-ui",
    "material design",
    "symbols",
    "icons"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/RubtsovAV/mui-symbols-material"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RubtsovAV/mui-symbols-material/issues"
  },
  "homepage": "https://github.com/RubtsovAV/mui-symbols-material",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "proptypes": "tsx ./scripts/generateProptypes.ts",
    "deduplicate": "pnpm dedupe",
    "clean": "lerna run --scope \"@mui-symbols-material/*\" clean",
    "src:download": "lerna run --scope \"@mui-symbols-material/*\" src:download",
    "src:icons": "lerna run --scope \"@mui-symbols-material/*\" src:icons",
    "build": "lerna run --scope \"@mui-symbols-material/*\" build",
    "eslint": "eslint . --cache --report-unused-disable-directives --ext .js,.ts,.tsx --max-warnings 0",
    "eslint:ci": "eslint . --report-unused-disable-directives --ext .js,.ts,.tsx --max-warnings 0",
    "stylelint": "stylelint --reportInvalidScopeDisables --reportNeedlessDisables \"docs/**/*.{js,ts,tsx}\"",
    "prettier": "pretty-quick --ignore-path .eslintignore",
    "prettier:all": "prettier --write . --ignore-path .eslintignore",
    "size:snapshot": "node --max-old-space-size=4096 ./scripts/sizeSnapshot/create",
    "size:why": "pnpm size:snapshot --analyze",
    "test": "node scripts/test.mjs",
    "tc": "node test/cli.js",
    "test:extended": "pnpm eslint && pnpm typescript",
    "typescript": "lerna run --no-bail --parallel typescript",
    "typescript:ci": "lerna run --concurrency 5 --no-bail --no-sort typescript",
    "validate-declarations": "tsx scripts/validateTypescriptDeclarations.mts",
    "generate-codeowners": "node scripts/generateCodeowners.mjs",
    "release:version": "node scripts/bumpVersion.js",
    "release:build": "pnpm src:download && pnpm src:icons && pnpm build",
    "release:publish": "pnpm publish --recursive --tag latest --filter \"@mui-symbols-material/*\""
  },
  "dependencies": {},
  "devDependencies": {
    "@argos-ci/core": "^1.5.3",
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/node": "^7.23.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-object-assign": "^7.23.3",
    "@babel/plugin-transform-react-constant-elements": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/register": "^7.23.7",
    "@mnajdova/enzyme-adapter-react-18": "^0.2.0",
    "@next/eslint-plugin-next": "^14.1.0",
    "@octokit/rest": "^20.0.2",
    "@playwright/test": "1.41.2",
    "@types/enzyme": "^3.10.18",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.14.202",
    "@types/mocha": "^10.0.6",
    "@types/node": "^18.19.15",
    "@types/prettier": "^2.7.3",
    "@types/react": "^18.2.55",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "babel-loader": "^9.1.3",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-optimize-clsx": "^2.6.2",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "chalk": "^5.3.0",
    "compression-webpack-plugin": "^11.0.0",
    "concurrently": "^8.2.2",
    "cpy-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "danger": "^11.3.1",
    "enzyme": "^3.11.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-webpack": "^0.13.8",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-mocha": "^10.2.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "globby": "^14.0.0",
    "karma": "^6.4.2",
    "karma-browserstack-launcher": "~1.6.0",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-firefox-launcher": "^2.1.2",
    "karma-mocha": "^2.0.1",
    "karma-sourcemap-loader": "^0.4.0",
    "karma-webpack": "^5.0.0",
    "lerna": "^8.0.2",
    "lodash": "^4.17.21",
    "markdownlint-cli2": "^0.12.1",
    "mocha": "^10.2.0",
    "nx": "^17.2.8",
    "nyc": "^15.1.0",
    "piscina": "^4.3.1",
    "postcss-styled-syntax": "^0.6.4",
    "prettier": "^3.2.5",
    "pretty-quick": "^4.0.0",
    "process": "^0.11.10",
    "raw-loader": "4.0.2",
    "rimraf": "^5.0.5",
    "serve": "^14.2.1",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "terser-webpack-plugin": "^5.3.10",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "webpack": "^5.90.1",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "yargs": "^17.7.2"
  },
  "packageManager": "pnpm@8.15.1",
  "resolutions": {
    "@babel/core": "^7.23.9",
    "@babel/code-frame": "^7.23.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-numeric-separator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-transform-destructuring": "npm:@minh.nguyen/plugin-transform-destructuring@^7.5.2",
    "@babel/plugin-transform-runtime": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/runtime": "^7.23.9",
    "@babel/types": "^7.23.9",
    "@definitelytyped/header-parser": "^0.2.3",
    "@definitelytyped/typescript-versions": "^0.1.0",
    "@definitelytyped/utils": "^0.1.2",
    "@types/node": "^18.19.15",
    "@types/react": "^18.2.55",
    "@types/react-dom": "18.2.19",
    "cross-fetch": "^4.0.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "nyc": {
    "include": [
      "packages/mui*/src/**/*.{js,ts,tsx}"
    ],
    "exclude": [
      "**/*.test.{js,ts,tsx}",
      "**/*.test/*"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
